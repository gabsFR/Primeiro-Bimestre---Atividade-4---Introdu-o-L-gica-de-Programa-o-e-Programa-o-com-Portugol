programa {
  inteiro nota1
    inteiro nota2
    inteiro nota3
    inteiro nota4
    inteiro media
  funcao inicio() {
  escreva("me fale suas notas e calcularei sua média ")
  leia(nota1)
  leia(nota2)
  leia(nota3)
  leia(nota4)
  media = (nota1 + nota2 + nota3 + nota4)
  escreva("sua média anual é: ", media / 4)
  }
}
