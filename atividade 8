programa {
  inteiro fish
  inteiro peso
  real multa, valor_multa
  inteiro sobras
  
  funcao inicio() {
    escreva ("manda os peixe aÃ­ ")
    leia(fish)
    leia(peso)
    multa = 4.50
    sobras = peso - 50
    
    se(peso >= 50){
    	valor_multa = multa * sobras
    }
    
    escreva("peixes: ",fish,"\n", "peso: ", peso,"\n", "excesso: ", sobras, "\n", "multa: ", valor_multa)
  }
}
